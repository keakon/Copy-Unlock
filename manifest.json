{
   "name": "Enable Copy",
   "version": "1.10",
   "manifest_version": 2,
   "description": "Allow selecting, copying, pasting and right clicking in some restricted pages.",
   "icons": {
      "19": "icon19.png",
      "128": "icon128.png"
   },
   "background": {
      "scripts": ["background.js"]
   },
   "browser_action": {
      "default_icon": "icon19-disable.png",
      "default_title": "Enable Copy"
   },
   "permissions": [ "tabs", "<all_urls>" ]
}
